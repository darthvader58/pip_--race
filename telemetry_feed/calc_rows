import fastf1

fastf1.Cache.enable_cache('fastf1_cache')
session = fastf1.get_session(2023, "Monaco", "R")
session.load()

laps = session.laps[session.laps['Team'] == "Williams"]

total_rows = 0
for _, lap in laps.iterrows():
    tel = lap.get_car_data().add_distance()
    total_rows += len(tel)

print(f"Total telemetry rows for Williams: {total_rows}")